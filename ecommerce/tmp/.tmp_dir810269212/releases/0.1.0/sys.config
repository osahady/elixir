[{distillery,[{config_providers,[]}]},
 {sasl,[{errlog_type,error},{sasl_error_logger,false}]},
 {swoosh,[{api_client,false}]},
 {esbuild,
     [{version,<<"0.14.29">>},
      {default,
          [{args,
               [<<"js/app.js">>,<<"--bundle">>,<<"--target=es2017">>,
                <<"--outdir=../priv/static/assets">>,
                <<"--external:/fonts/*">>,<<"--external:/images/*">>]},
           {cd,<<"d:/05_web/elixir/ecommerce/assets">>},
           {env,
               #{<<"NODE_PATH">> =>
                     <<"d:/05_web/elixir/ecommerce/deps">>}}]}]},
 {phoenix,[{json_library,'Elixir.Jason'}]},
 {ecommerce,
     [{ecto_repos,['Elixir.Ecommerce.Repo']},
      {'Elixir.Ecommerce.Mailer',[{adapter,'Elixir.Swoosh.Adapters.Local'}]},
      {'Elixir.EcommerceWeb.Endpoint',
          [{render_errors,
               [{view,'Elixir.EcommerceWeb.ErrorView'},
                {accepts,[<<"html">>,<<"json">>]},
                {layout,false}]},
           {pubsub_server,'Elixir.Ecommerce.PubSub'},
           {live_view,[{signing_salt,<<"wk5ZMcEz">>}]},
           {server,true},
           {load_from_system_env,true},
           {http,[{port,{system,<<"PORT">>}}]},
           {secret_key_base,<<"${SECRET_KEY_BASE}">>},
           {url,[{host,<<"${APP_NAME}.gigalixirapp.com">>},{port,443}]},
           {cache_static_manifest,<<"priv/static/cache_manifest.json">>},
           {version,<<"0.1.0">>},
           {cache_static_manifest,<<"priv/static/cache_manifest.json">>}]}]},
 {logger,
     [{console,
          [{format,<<"$time $metadata[$level] $message\n">>},
           {metadata,[request_id]}]},
      {level,info}]}].
